<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.firstandroid.snakegameapp">

    <application
        android:allowBackup="true"
        android:icon="@drawable/iconsnake"
        android:label="@string/app_name"
        android:roundIcon="@drawable/iconsnakeround"
        android:screenOrientation="portrait"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".LeaderboardActivity"></activity>
        <activity android:name=".UsernameActivity" />
        <activity android:name=".SettingsActivity" />
        <activity android:name=".StartingActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".MainActivity" />

        <meta-data
            android:name="com.google.android.gms.ads.APPLICATION_ID"
            android:value="ca-app-pub-5961795793039298~9291204846" />
    </application>

</manifest>